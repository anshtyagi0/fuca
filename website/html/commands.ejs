    <body>
      <div class="container">
        <div id="accordion">
          <% for(const x of commandsname) { %> 
          <% let commandname =
          x; %> <% let commanddescription = commandsdescription[x];
          %>
          <div class="card">
            <div class="card-header">
              <a
                class="card-link"
                data-toggle="collapse"
                href="#collapse<%=x %>"
              >
                /<%= commandname %>
              </a>
            </div>
            <div
              id="collapse<%=x %>"
              class="collapse show"
              data-parent="#accordion"
            >
              <div class="card-body"><%= commanddescription %></div>
            </div>
          </div>
          <% } %>
        </div>
      </div>
    </body>
