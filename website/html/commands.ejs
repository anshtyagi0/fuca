    <script>
      const URLParams = new URLSearchParams(window.location.search);
      const isLogged = URLParams.get("login");
      if (isLogged === "true") {
        try {
          window.close();
        } catch {}
      }
      function openWindow() {
        var win = window.open(
          "/login",
          "Login",
          "width=550,height=800,left=450"
        );
        var timer = setInterval(function () {
          if (win.closed) {
            clearInterval(timer);
            check();
          }
        }, 1000);
      }
      function check() {
        location.reload();
      }
    </script>
